<maui:MauiWinUIApplication
    x:Class="ModUlar.WinUI.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:maui="using:Microsoft.Maui"
    xmlns:local="using:ModUlar.WinUI">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.ThemeDictionaries>
                <ResourceDictionary x:Key="Dark">
                    <SolidColorBrush x:Key="WindowCaptionBackground" Color="#111111"/>
                    <SolidColorBrush x:Key="WindowCaptionBackgroundDisabled" Color="#111111"/>
                    <SolidColorBrush x:Key="WindowCaptionForeground" Color="#FFFF"/>
                    <SolidColorBrush x:Key="WindowCaptionForegroundDisabled" Color="#AFFF"/>
                </ResourceDictionary>
            </ResourceDictionary.ThemeDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</maui:MauiWinUIApplication>
